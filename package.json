{
  "name": "edtronics",
  "version": "1.0.0",
  "engines": {
    "node": "8.4.0"
  },
  "private": true,
  "description": "This is a eductional technology company that specilize in hardware and software for teachers students schools and parents.",
  "main": "index.js ",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "heroku-prebuild": "bash ./scripts/deployment_react.sh",
    "heroku-postbuild": "bash ./scripts/deployment_cleanup.sh",
    "test": "mocha || true",
    "dev": "nodemon",
    "key": "ssh-add",
    "rebuild": "webpack -d --progress --colors",
    "watch": "webpack -d --progress --watch",
    "start": "node index.js || true",
    "debug-server": "nodemon --debug server.js",
    "debug": "node-inspector server.js",
    "db": "psql Edtronics",
    "heroku": "git push heroku master",
    "heroku-schema": "heroku pg:psql --app edtronics < db/schema.sql",
    "heroku-seed": "heroku pg:psql --app edtronics < db/seed.sql",
    "heroku-logs": "heroku logs --tail",
    "heroku-local": "heroku local web",
    "heroku-papertrail": "heroku addons:open papertrail",
    "heroku-db": "heroku pg:psql",
    "git": "git add . && git commit -m 'File updated in [Edtronics] application folder' && git push",
    "git-heroku": "git add . && git commit -m 'File updated in [Edtronics] application folder' && git push && git push heroku master"
  },
  "author": "Mohamed Gassama & Tenzin",
  "license": "MIT ",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "helmet": "^3.8.2",
    "immutable": "^3.8.2",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.5",
    "mocha": "^3.5.3",
    "morgan": "^1.9.0",
    "node-fetch": "^2.0.0-alpha.9",
    "nodemon": "^1.12.1",
    "pg-promise": "^5.5.1"
  }
}
